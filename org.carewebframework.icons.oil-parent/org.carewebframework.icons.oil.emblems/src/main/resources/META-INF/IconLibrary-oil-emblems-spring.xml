<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<beans profile="root">

		<bean class="org.carewebframework.ui.icon.IconLibraryBase">
			<constructor-arg value="emblems" />
			<constructor-arg value="/org/carewebframework/icons/oil/" />
			<constructor-arg value="16x16,22x22,24x24,32x32,48x48,64x64,128x128,8x8" />
		</bean>

	</beans>

</beans>

